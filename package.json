{
  "name": "uninspected",
  "version": "1.10.0",
  "description": "util.inspect replacement based on Unexpected's output and type system",
  "main": "./lib/uninspected",
  "dependencies": {
    "magicpen-prism": "^2.4.0",
    "unexpected": "^10.39.1"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "bundle-collapser": "^1.3.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.8.0",
    "eslint-config-pretty-standard": "^2.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.14.3",
    "sinon": "^1.17.1",
    "uglify-js": "^2.6.2",
    "unexpected-sinon": "^10.2.1"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "mocha",
    "ci": "npm test && npm run lint && npm run coverage",
    "coverage": "NODE_ENV=development nyc --reporter=lcov --reporter=text --all -- npm test && echo google-chrome coverage/lcov-report/index.html",
    "browserify": "browserify -p bundle-collapser/plugin -e ./lib/uninspected.js -s uninspected -b > uninspected.js",
    "bookmarklet": "(echo -n '<!DOCTYPE html><html><body><a href=\"javascript:' && < uninspected.js uglifyjs -m -c | sed -e 's/&/\\&amp;/g;' | sed -e 's/\"/\\&quot;/g;' | tr --delete '\\n' && echo -n ';console=uninspected;void(null);\">Replace the console object with uninspected</a></body></html>') > bookmarklet.html",
    "prepublish": "npm run browserify && npm run bookmarklet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/unexpectedjs/uninspected.git"
  },
  "keywords": [
    "util",
    "inspect",
    "object",
    "depth",
    "color",
    "types"
  ],
  "author": "Andreas Lind <andreaslindpetersen@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/unexpectedjs/uninspected/issues"
  },
  "homepage": "https://github.com/unexpectedjs/uninspected",
  "nyc": {
    "include": [
      "lib/**"
    ]
  }
}
