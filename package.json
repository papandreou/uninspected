{
  "name": "uninspected",
  "version": "1.5.0",
  "description": "util.inspect replacement based on Unexpected's output and type system",
  "main": "./lib/uninspected",
  "dependencies": {
    "magicpen": "4.2.0",
    "magicpen-prism": "1.0.0",
    "unexpected": "5.5.0"
  },
  "devDependencies": {
    "browserify": "^5.9.1",
    "bundle-collapser": "^1.1.0",
    "coveralls": "2.11.2",
    "istanbul": "0.3.2",
    "jshint": "2.5.10",
    "mocha": "2.0.1",
    "sinon": "1.12.1",
    "unexpected-sinon": "5.0.0"
  },
  "scripts": {
    "lint": "jshint .",
    "test": "mocha && npm run lint",
    "travis": "npm test && npm run coverage && <coverage/lcov.info coveralls",
    "coverage": "NODE_ENV=development istanbul cover _mocha -- --reporter dot",
    "browserify": "browserify -p bundle-collapser/plugin -e ./lib/uninspected.js -s uninspected -b > uninspected.js",
    "bookmarklet": "(echo -n '<!DOCTYPE html><html><body><a href=\"javascript:' && < uninspected.js uglifyjs -m -c | sed -e 's/&/\\&amp;/g;' | sed -e 's/\"/\\&quot;/g;' | tr --delete '\\n' && echo -n ';console=uninspected;void(null);\">Bookmarklet: Replace the console object with uninspected</a></body></html>') > bookmarklet.html",
    "prepublish": "npm run browserify && npm run bookmarklet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/papandreou/uninspected.git"
  },
  "keywords": [
    "util",
    "inspect",
    "object",
    "depth",
    "color",
    "types"
  ],
  "author": "Andreas Lind <andreas@one.com>",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/papandreou/uninspected/issues"
  },
  "homepage": "https://github.com/papandreou/uninspected"
}
